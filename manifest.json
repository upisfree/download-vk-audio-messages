{
  "name": "VK audio messages downloader",
  "description": "Arina asked me to write this.",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "downloads", "https://vk.com/im*"
  ],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["background.js"]
  }],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon16.png"
  }
}